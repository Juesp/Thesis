<canvas id="myChart" style="position: relative; height: 62vh; width: 80vw;"></canvas>
<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

<script>
    var ctx = document.getElementById('myChart')

    const unixDate = [];
    for (i=0; i<[{{fechas}}].length; i++){
        unixDate.push(new Date([{{fechas}}][i]).toLocaleDateString());
    }
    var myChart = new Chart(ctx, {
        type: 'line',
        data:{
            labels: unixDate,
            //console.log(fechas)
            datasets: [{
                label: 'Seguimiento de resultados por prueba aplicada',
                data: [{{resultados}}],
                backgroundColor: ['#6bf1ab', '#63d69f', '#438c6c', '#509c7f', '#1f794e', '#34444c', '#90CAF9', '#64B5F6', '#42A5F5', '#2196F3', '#0D47A1'],
                borderColor: 'rgb(75, 192, 192)',
                tension: 0.1
            }]
        },
        options: {
            scales: {
                y:{
                    beginAtZero:true
                }
            }
        }

    })
    console.log(myChart.data)
</script>