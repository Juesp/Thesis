<canvas id="myChart" style="position: relative; height: 62vh; width: 80vw;"></canvas>
<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

<script>
    var ctx = document.getElementById('myChart')

    var myChart = new Chart(ctx, {
        type: 'bar',
        data:{
            labels: ['Hasta 20 años', 'De 20 a 40 años', 'De 40 a 60 años', 'De 60 a 80 años', 'De 80 años en adelante'],
            //console.log(fechas)
            datasets: [{
                label: 'Promedio de los resultados en pruebas aplicadas por grupo de edades (ug/mL)',
                data: [{{promUntil20}}, {{promUntil40}}, {{promUntil60}}, {{promUntil80}}, , {{promUntil90}}],
                backgroundColor: [
                'rgba(54, 162, 235, 0.2)',
                'rgba(255, 206, 86, 0.2)',
                'rgba(75, 192, 192, 0.2)',
                'rgba(153, 102, 255, 0.2)',
                'rgba(255, 159, 64, 0.2)'
                ],
                borderColor: [
                'rgba(54, 162, 235, 1)',
                'rgba(255, 206, 86, 1)',
                'rgba(75, 192, 192, 1)',
                'rgba(153, 102, 255, 1)',
                'rgba(255, 159, 64, 1)'
                ],
                borderWidth: 1
            }]
        },
        options: {
            scales: {
                y:{
                    beginAtZero:true
                }
            }
        }

    })
    console.log(myChart.data)
</script>